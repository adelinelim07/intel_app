<p id="notice"><%= notice %></p>

<h1>Intel Companies</h1>

<table>
  <thead>
    <tr>
      <th>Intel</th>
      <th>Company</th>
      <th>Qty</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @intel_companies.each do |intel_company| %>
      <tr>
        <td><%= intel_company.intel_id %></td>
        <td><%= intel_company.company_id %></td>
        <td><%= intel_company.qty %></td>
        <td><%= link_to 'Show', intel_company %></td>
        <td><%= link_to 'Edit', edit_intel_company_path(intel_company) %></td>
        <td><%= link_to 'Destroy', intel_company, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Intel Company', new_intel_company_path %>
